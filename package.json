{
  "name": "bpmn4es",
  "version": "0.1.0",
  "description": "An extension of bpmn-js that supports the BPMN4ES notation.",
  "scripts": {
    "all": "run-s lint build",
    "build": "webpack --mode production",
    "build:watch": "webpack --watch",
    "dev": "run-p build:watch serve",
    "serve": "sirv public --dev --host 0.0.0.0",
    "lint": "eslint .",
    "start": "run-s build serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michel-medema/BPMN4ES"
  },
  "keywords": [
    "bpmn4es",
    "environmental sustainability"
  ],
  "author": {
    "name": "Michel Medema",
    "url": "https://github.com/michel-medema"
  },
  "license": "MIT",
  "devDependencies": {
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^9.21.0",
    "eslint-plugin-bpmn-io": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "sirv-cli": "^3.0.1",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "bpmn-js": "^18.3.1",
    "express": "^4.21.2",
    "file-saver": "^2.0.5"
  }
}
